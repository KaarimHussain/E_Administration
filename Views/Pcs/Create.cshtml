@model E_Administration.Models.Pc

@{
    ViewData["Title"] = "Create";
}
<div class="container py-5">

    <h1 class="display-3">Create</h1>

    <h4>Pc</h4>
    <hr />
    <div class="row">
        <div class="col-md-4">
            <form asp-action="Create">
                <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                <div class="form-group">
                    <label asp-for="SoftId" class="control-label"></label>
                    <select asp-for="SoftId" class="form-control" asp-items="ViewBag.SoftId"></select>
                </div>
                <div class="form-group">
                    <label asp-for="HardId" class="control-label"></label>
                    <select asp-for="HardId" class="form-control" asp-items="ViewBag.HardId"></select>
                </div>
                <div class="form-group">
                    <label asp-for="LabId" class="control-label"></label>
                    <select asp-for="LabId" class="form-control" asp-items="ViewBag.LabId"></select>
                </div>
                <div class="form-group">
                    <label asp-for="PcName" class="control-label"></label>
                    <input asp-for="PcName" class="form-control" />
                    <span asp-validation-for="PcName" class="text-danger"></span>
                </div>
                <br />
                <div class="form-group">
                    <input type="submit" value="Create" class="btn btn-primary" />
                </div>
            </form>
        </div>
    </div>
    <br />
    <div>
        <a asp-action="Index">Back to List</a>
    </div>
</div>
@section Scripts {
    @{
        await Html.RenderPartialAsync("_ValidationScriptsPartial");
    }
}
